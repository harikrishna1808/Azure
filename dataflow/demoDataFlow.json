{
	"name": "demoDataFlow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "srcBlob"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "tblsalgt26k",
						"type": "DatasetReference"
					},
					"name": "tblsalgt26k"
				},
				{
					"dataset": {
						"referenceName": "tablsallt26k",
						"type": "DatasetReference"
					},
					"name": "tablsallt26k"
				}
			],
			"transformations": [
				{
					"name": "addIncrementColumn"
				},
				{
					"name": "split1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          EMP_ID as integer,",
				"          EMP_FIRST_NM as string,",
				"          EMP_LAST_NM as string,",
				"          EMP_EMAIL_ID as string,",
				"          EMP_PHONE_NR as long,",
				"          EMP_SAL as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> srcBlob",
				"srcBlob derive(increment = EMP_SAL + 10000) ~> addIncrementColumn",
				"addIncrementColumn split(EMP_SAL >= 26000,",
				"     EMP_SAL < 26000,",
				"     disjoint: true) ~> split1@(salarygt26ksplit, salarylt26ksplit)",
				"split1@salarygt26ksplit sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> tblsalgt26k",
				"split1@salarylt26ksplit sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          EMP_ID as integer,",
				"          EMP_FIRST_NM as string,",
				"          EMP_LAST_NM as string,",
				"          EMP_EMAIL_ID as string,",
				"          EMP_PHONE_NR as long,",
				"          EMP_SAL as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          EMP_ID,",
				"          EMP_FIRST_NM,",
				"          EMP_LAST_NM,",
				"          EMP_EMAIL_ID,",
				"          EMP_PHONE_NR,",
				"          EMP_SAL",
				"     )) ~> tablsallt26k"
			]
		}
	}
}