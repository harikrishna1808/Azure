{
	"name": "demoDataFlow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "srcBlob"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          EMP_ID as integer,",
				"          EMP_FIRST_NM as string,",
				"          EMP_LAST_NM as string,",
				"          EMP_EMAIL_ID as string,",
				"          EMP_PHONE_NR as integer,",
				"          EMP_SAL as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> srcBlob"
			]
		}
	}
}